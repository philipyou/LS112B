format
  ! 0x0e jam
  % immediate
  % addr
  [5] opcode
  [3] 0
  [1] 0   /* msb */
  [15] addr
  [8] immediate
end

format
  ! 0x0e hjam
  % immediate
  % addr
  [5] opcode
  [3] 0
  [1] 1   /* msb */
  [15] addr
  [8] immediate
end

format			/* 0x242... */
  ! 4 nbranch
  % addr
  % flag
  [5] opcode
  [1] 1
  [5] 1
  [6] flag
  [15] addr
end

format
  ! 4 ncall
  % addr
  % flag
  [5] opcode
  [1] 1
  [5] 2
  [6] flag
  [15] addr
end

format
  ! 4 nrtn
  % flag
  [5] opcode
  [1] 1
  [5] 3
  [6] flag
  [15] 0
end

format
  ! 4 nop
  % addr  /* no-op for addr+2 clocks */
  [5] opcode
  [6] 0     
  [6] 0     /* always */ 
  [15] addr
end

format
  ! 4 clear_stack
  [5] opcode
  [6] 4
