  [6] regr
  [6] 5
  [6] regw
  [9] immediate
end

format
  ! 3 xor_into
  % immediate
  % reg
  [5] opcode
  [6] reg
  [6] 5
  [6] reg
  [9] immediate
end
  
format
  ! 0x13 ixor  /* pdata xor regr and store result in regw */
  % regr
  % regw
  [5] opcode
  [6] regr
  [6] 5
  [6] regw
  [9] 0
end
  
format
  ! 3 invert
  % regr
  % regw
  [5] opcode
  [6] regr
  [6] 8
  [6] regw
  [9] 0
end

format
  ! 3 pinvert    /* invert pdata into itself */
  [5] opcode
